{
  "name": "platform-backend",
  "version": "1.0.0",
  "main": "build/shared/infra/http/server.js",
  "repository": "https://github.com/sLinked/platform-backend",
  "author": "Marcos Proença, Tiago Gonçalves",
  "license": "MIT",
  "prisma": {
    "seed": "node build/shared/infra/prisma/seeders"
  },
  "scripts": {
    "dev:server": "tsnd -r tsconfig-paths/register --transpile-only --inspect --ignore-watch node_modules src/shared/infra/http/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files",
    "test:unit": "jest --coverage --detectOpenHandles ./src/modules/**/__tests__/unit/**.spec.ts",
    "test:integration": "cross-env NODE_ENV=test jest --detectOpenHandles ./src/modules/**/__tests__/integration/**.spec.ts",
    "test": "yarn test:integration && yarn test:unit",
    "start": "cross-env NODE_ENV=production node build/shared/infra/http/server.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.335.0",
    "@prisma/client": "^4.14.1",
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "firebase-admin": "^11.8.0",
    "jsonwebtoken": "^9.0.0",
    "mime": "^3.0.0",
    "multer": "1.4.5-lts.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^4.2.0",
    "tsyringe": "^4.7.0",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mime": "^3.0.1",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "prisma": "^4.14.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
